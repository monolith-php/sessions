{
  "name": "monolith/web-sessions",
  "description": "Web sessions component for the Monolith framework.",
  "keywords": [
    "monolith",
    "sessions"
  ],
  "require": {
    "php": ">=7.4",
    "monolith/collections": "^8",
    "monolith/configuration": "^6",
    "monolith/component-bootstrapping": "^7",
    "monolith/dependency-injection": "^8",
    "monolith/http": "^11",
    "monolith/web-routing": "^22",
    "predis/predis": "^2.0@dev",
    "ramsey/uuid": "^4"
  },
  "require-dev": {
    "phpspec/phpspec": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\WebSessions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\WebSessions\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
