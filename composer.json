{
  "name": "monolith/web-sessions",
  "description": "Web sessions component for the Monolith framework.",
  "keywords": [
    "monolith",
    "sessions"
  ],
  "require": {
    "php": ">=8",
    "monolith/collections": "^9",
    "monolith/configuration": "^7",
    "monolith/component-bootstrapping": "^8",
    "monolith/dependency-injection": "^9",
    "monolith/http": "^12",
    "monolith/web-routing": "^23",
    "predis/predis": "^2.0@dev",
    "ramsey/uuid": "^4"
  },
  "require-dev": {
    "phpspec/phpspec": "^7"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.humanmusic.eu",
      "canonical": false,
      "only": [
        "monolith/*"
      ]
    }
  ],
  "autoload": {
    "psr-4": {
      "Monolith\\WebSessions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\WebSessions\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
