{
  "name": "monolith/web-sessions",
  "description": "Web sessions component for the Monolith framework.",
  "keywords": [
    "monolith",
    "sessions"
  ],
  "require": {
    "php": ">=8.1",
    "monolith/collections": "^10",
    "monolith/configuration": "^8",
    "monolith/component-bootstrapping": "^9",
    "monolith/dependency-injection": "^10",
    "monolith/http": "^13",
    "monolith/web-routing": "^24",
    "predis/predis": "^2.0@dev",
    "ramsey/uuid": "^4"
  },
  "require-dev": {
    "phpspec/phpspec": "^7"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\WebSessions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\WebSessions\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
