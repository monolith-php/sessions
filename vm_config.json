{
  "ansible_python_interpreter": "auto_legacy_silent",
  "hostname": "web-sessions",
  "dbuser": "root",
  "dbpasswd": "password",
  "databases": [
    "development",
    "testing"
  ],
  "sites": [
    {
      "hostname": "app.local",
      "document_root": "/vagrant/public"
    }
  ],
  "php_configs": [
    {
      "option": "upload_max_filesize",
      "value": "100M"
    },
    {
      "option": "post_max_size",
      "value": "100M"
    }
  ],
  "boost_pam_limits": "yes",
  "boosted_pam_limit": 40000,
  "install_redis": "yes",
  "install_db": "no",
  "install_nginx": "no",
  "install_php": "yes",
  "php_version": "8.0",
  "enable_swap": "yes",
  "swap_size_in_mb": "1024",
  "install_ohmyzsh": "yes"
}
